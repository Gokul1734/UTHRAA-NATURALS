{
 "buildCommand": "npm run build",
 "outputDirectory": "dist",
 "framework": "vite",
 "rewrites": [
  {
   "source": "/(.*)",
   "destination": "/index.html"
  }
 ],
 "headers": [
  {
   "source": "/assets/(.*)",
   "headers": [
    {
     "key": "Cache-Control",
     "value": "public, max-age=31536000, immutable"
    }
   ]
  }
 ],
 "env": {
  "REACT_APP_API_URL": "@react_app_api_url"
 }
}